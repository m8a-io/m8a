{
  "name": "@m8a/zeus-app",
  "version": "0.0.1",
  "private": true,
  "description": "A starter kit for front end work with m8a",
  "keywords": [
    "platform",
    "PaaS",
    "SaaS",
    "TypeScript"
  ],
  "author": "Scott Molinari <scott.molinari@adduco.de>",
  "scripts": {
    "build": "quasar build",
    "codegen": "tsc src/graphql/utils/operations-from-schema.ts && graphql-codegen --config codegen.yml",
    "lint": "eslint --ext .js,.ts,.vue ./ --fix",
