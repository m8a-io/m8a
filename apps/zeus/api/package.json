{
  "name": "@m8a/zeus-api",
  "version": "0.0.1",
  "private": true,
  "description": "The 'god of gods' api for m8a",
  "keywords": [
    "platform",
    "PaaS",
    "SaaS",
    "TypeScript"
  ],
  "license": "MIT",
  "author": "Scott Molinari <scott.molinari@m8a.io>",
  "scripts": {
    "_phase:build": "pnpm prebuild && swc src -d dist --config-file .build.swcrc",
    "_phase:test": "jest --detectOpenHandles",
    "prebuild": "rimraf dist",
    "build": "pnpm prebuild && swc src -d dist --config-file .build.swcrc",
    "dev": "nest start --builder swc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:debug": "nest start --builder swc --debug --watch",
    "start:dev": "nest start --builder swc --watch",
    "start:prod": "node dist/main",
    "test": "jest --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.1",
    "@m8a/core": "workspace:*",
    "@m8a/logger": "workspace:*",
    "@nestjs/common": "10.4.5",
    "@nestjs/core": "10.4.5",
    "@nestjs/platform-fastify": "10.4.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1"
  },
  "devDependencies": {
    "@m8a/eslint-config": "workspace:*",
    "@nestjs/cli": "10.4.5",
    "@nestjs/testing": "10.4.5",
    "@rushstack/eslint-config": "4.0.2",
    "@swc/cli": "~0.4.0",
    "@swc/core": "~1.7.0",
    "@types/jest": "29.5.13",
    "@types/node": "20.16.15",
    "@types/supertest": "6.0.2",
    "eslint": "9.13.0",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "reflect-metadata": "0.2.2",
    "rimraf": "~6.0.0",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-json-schema-generator": "2.3.0",
    "ts-loader": "9.5.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3",
    "webpack": "5.95.0"
  },
  "peerDependenciesMeta": {
    "@m8a/core": {
      "injected": true
    }
  }
}
