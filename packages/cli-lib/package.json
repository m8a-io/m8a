{
  "name": "@m8a/cli-lib",
  "version": "0.0.1",
  "description": "This is a package library of standard m8a CLI modules, which are used both for the global CLI and the local CLI packages. It is necessary to avoid code duplication between the two CLI packages.",
  "keywords": [
    "m8a",
    "nodejs",
    "nestjs"
  ],
  "license": "MIT",
  "author": "Scott Molinari",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@m8a/logger": "workspace:*",
    "cross-spawn": "7.0.3"
  },
  "devDependencies": {
    "@rushstack/eslint-config": "3.3.2",
    "@types/cross-spawn": "6.0.2",
    "@types/node": "18.16.19",
    "eslint": "8.45.0",
    "rimraf": "5.0.1",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0"
  }
}
