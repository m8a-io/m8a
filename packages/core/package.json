{
  "name": "@m8a/core",
  "version": "0.0.6",
  "description": "The core modules for m8a applications.",
  "keywords": [
    "platform",
    "PaaS",
    "SaaS",
    "TypeScript"
  ],
  "homepage": "https://github.com/m8a-io/m8a#readme",
  "bugs": {
    "url": "https://github.com/m8a-io/m8a/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m8a-io/m8a.git"
  },
  "license": "MIT",
  "author": "Scott Molinari",
  "main": "dist/index.js",
  "types": "dist",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "pnpm prebuild && tsc --project tsconfig.build.json",
    "build:watch": "pnpm prebuild && tsc --project tsconfig.build.json --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint --fix ./",
    "test": "jest --detectOpenHandles",
    "test:ci": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@casl/ability": "~6.3.0",
    "@envelop/core": "3.0.6",
    "@graphql-yoga/nestjs": "0.3.1",
    "@m8a/nestjs-typegoose": "workspace:*",
    "@nestjs/graphql": "10.2.0",
    "@nestjs/jwt": "10.0.2",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-fastify": "9.3.12",
    "@ogma/nestjs-module": "4.2.2",
    "@ptc-org/nestjs-query-core": "2.1.1",
    "@ptc-org/nestjs-query-graphql": "2.1.1",
    "@ptc-org/nestjs-query-typegoose": "2.1.1",
    "@typegoose/typegoose": "10.3.4",
    "apollo-server-errors": "3.3.1",
    "argon2": "0.30.3",
    "axios": "1.3.4",
    "cache-manager": "5.1.7",
    "cache-manager-redis-store": "3.0.1",
    "camel-case": "4.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "fastify": "4.15.0",
    "graphql": "16.6.0",
    "mercurius": "10.5.1",
    "mongoose": "6.10.4",
    "mongoose-autopopulate": "0.17.1",
    "ms": "3.0.0-canary.1",
    "nest-typed-config": "~2.5.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1"
  },
  "devDependencies": {
    "@apollo/gateway": "2.3.2",
    "@m8a/eslint-config": "workspace:*",
    "@nestjs/common": "9.3.12",
    "@nestjs/core": "9.3.12",
    "@nestjs/mercurius": "^10.1.0",
    "@nestjs/testing": "9.3.12",
    "@rushstack/eslint-config": "3.2.0",
    "@types/cache-manager": "4.0.2",
    "@types/cache-manager-redis-store": "2.0.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.6",
    "@types/passport-jwt": "3.0.8",
    "dataloader": "2.2.2",
    "eslint": "8.35.0",
    "graphql-subscriptions": "2.0.0",
    "jest": "29.4.1",
    "mercurius-integration-testing": "5.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.1.2",
    "rxjs": "7.8.0",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-morph": "^17.0.0",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "webpack": "5.75.0"
  },
  "peerDependenciesMeta": {
    "nestjs/typegoose": {
      "injected": true
    }
  },
  "overrides": {
    "class-validator": "0.14.0"
  }
}
