{
  "name": "@m8a/logger",
  "version": "0.0.1",
  "description": "The logger module for m8a applications.",
  "keywords": [
    "platform",
    "PaaS",
    "SaaS",
    "TypeScript"
  ],
  "homepage": "https://github.com/m8a-io/m8a#readme",
  "bugs": {
    "url": "https://github.com/m8a-io/m8a/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m8a-io/m8a.git"
  },
  "license": "MIT",
  "author": "Scott Molinari",
  "main": "dist/index.js",
  "types": "dist",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "pnpm prebuild && tsc --project tsconfig.build.json",
    "build:watch": "pnpm prebuild && tsc --project tsconfig.build.json --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint --fix ./",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },

  "dependencies": {
    "chalk-v2": "1.0.2",
    "rxjs": "7.8.0"
  },

  "devDependencies": {
    "@golevelup/nestjs-modules": "0.6.1",
    "@nestjs/common": "9.3.8",
    "@nestjs/core": "9.3.8",
    "@nestjs/platform-fastify": "9.3.8",
    "@ogma/nestjs-module": "4.2.2",
    "@ogma/platform-fastify": "4.0.1",
    "fastify": "4.12.0",
    "@nestjs/testing": "9.3.8",
    "@types/node": "18.11.18",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.1.2",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.2.0"
  }
}
