{
  "name": "@m8a/nestjs-typegoose",
  "version": "12.0.0",
  "description": "A nestjs module wrapper for typegoose",
  "homepage": "https://nestjs-typegoose.m8a.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m8a-io/m8a.git",
    "directory": "deps/nestjs-typegoose"
  },
  "license": "MIT",
  "author": "Kyle Pfromer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "_phase:build": "tsc -p tsconfig.build.json",
    "_phase:test": "jest --detectOpenHandles",
    "build": "tsc -p tsconfig.build.json",
    "commit": "git cz",
    "coverage": "yarn run test:cov && cat ./coverage/lcov.info | coveralls",
    "dev": "tsc -p tsconfig.build.json --watch",
    "lint": "eslint --fix ./",
    "prepare": "npm run build",
    "test": "jest  --verbose",
    "test:ci": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "resetMocks": true,
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "is-class": "0.0.9"
  },
  "devDependencies": {
    "@m8a/eslint-config": "workspace:*",
    "@nestjs/common": "10.4.5",
    "@nestjs/core": "10.4.5",
    "@nestjs/platform-express": "~10.4.0",
    "@nestjs/testing": "10.4.5",
    "@rushstack/eslint-config": "3.7.1",
    "@typegoose/typegoose": "12.8.0",
    "@types/jest": "29.5.13",
    "@types/supertest": "6.0.2",
    "coveralls": "3.1.1",
    "eslint": "9.13.0",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "mongodb-memory-server": "10.1.2",
    "mongoose": "8.7.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "superagent": "10.1.1",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@typegoose/typegoose": "^12.0.0",
    "mongoose": "^8.0.1"
  },
  "engines": {
    "node": ">=18"
  }
}
